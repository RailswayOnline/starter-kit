<h1>Forgot password</h1>
<p>Find me in app/views/password_resets/new.html.erb</p>

<%= form_for(:password_reset, url: password_resets_path ) do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class="actions">
    <%= f.submit 'Submit' %>
  </div>
<% end %>
